{
  "name": "@splunk/react-icons",
  "version": "3.2.0",
  "description": "Library of various React icons",
  "license": "Apache-2.0",
  "author": "Splunk Inc.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --bail && yarn types:build",
    "eslint": "eslint src --ext \".ts,.tsx\"",
    "eslint:fix": "eslint src --ext \".ts, .tsx\" --fix",
    "eslint:ci": "yarn run eslint -- -f junit -o test-reports/lint-results.xml",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "start": "cross-env NODE_ENV=development webpack --watch",
    "stylelint": "stylelint \"src/**/*.{ts,tsx}\" --config stylelint.config.js",
    "types:build": "tsc --emitDeclarationOnly --declaration",
    "types:start": "yarn run types:build --watch"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8",
    "styled-components": "5.1.1"
  },
  "dependencies": {
    "@splunk/ui-utils": "^1.4.0",
    "@types/lodash": "^4.14.156",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "lodash": "^4.17.14",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@splunk/babel-preset": "^3.0.0",
    "@splunk/eslint-config": "^4.0.0",
    "@splunk/stylelint-config": "^4.0.0",
    "@splunk/webpack-configs": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^6.0.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "5.1.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.0.5",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  },
  "engines": {
    "node": ">=6"
  }
}
