{
  "name": "@splunk/react-page",
  "version": "6.0.2",
  "description": "Load React components into the latest layout from Splunk Enterprise",
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "author": "Splunk Inc.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "docs": "NODE_ENV=production webpack --config docs.gen.webpack.config.js",
    "docs:publish": "cicd-publish-docs docs --force",
    "docs:start": "webpack serve --config docs.gen.webpack.config.js",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "eslint:ci": "yarn run eslint -- -f junit -o test-reports/lint-results.xml",
    "lint": "npm run eslint && npm run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "start": "cross-env NODE_ENV=development webpack --watch",
    "stylelint": "stylelint \"src/**/*.{js,jsx}\" --config stylelint.config.js",
    "test:functional:prepare": "node src/tests/helpers/prepare_environment.js 8.0.3",
    "test:functional": "yarn run test:functional:prepare && splunk-wdio-functional-test-runner functional.local.conf.js",
    "test:functional:ci": "yarn run test:functional:prepare && splunk-wdio-functional-test-runner functional.ci.conf.js",
    "test:functional:cloud": "yarn run test:functional:prepare && splunk-wdio-functional-test-runner functional.cloud.conf.js",
    "test:cypress": "echo TODO: SUI-3442",
    "test:cypress:ci": "echo TODO: SUI-3442"
  },
  "dependencies": {
    "@splunk/react-ui": "^4.12.0",
    "@splunk/splunk-utils": "^2.2.2",
    "@splunk/themes": "^0.13.0",
    "@splunk/ui-utils": "^1.5.0",
    "prop-types": "^15.6.2",
    "scriptjs": "^2.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@splunk/babel-preset": "^3.0.0",
    "@splunk/cicd-tools": "^0.4.0",
    "@splunk/eslint-config": "^4.0.0",
    "@splunk/stylelint-config": "^4.0.0",
    "@splunk/wdio-functional-test-runner": "^10.1.0",
    "@splunk/webpack-configs": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^6.0.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^0.23.1",
    "styled-components": "5.1.1",
    "stylelint": "^13.0.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8",
    "styled-components": "5.1.1"
  },
  "engines": {
    "node": ">=6"
  }
}
